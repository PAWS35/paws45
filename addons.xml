<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.IPTV"
       name="IPTV"
       version="3.0.28"
       provider-name="Uscclarkey">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>IPTV</summary>
    <description>Stream all content form my m3u lists. Remember to click the "!Update" button regularly to keep your list up to date. For more info and instructions on how to use this add-on please visit the IPTV section of our website www.streamboxlive.wordpress.com</description>
    <platform>all</platform>
  </extension>
</addon>

<addon
  id="plugin.video.SportsDevil"
  version="1.8.5.7"
  name="SportsDevil"
  provider-name="MaxMustermann">
  <requires>
   <import addon="xbmc.python" version="2.1.0"/>
   <import addon="script.common.plugin.cache" version="0.9.1"/>
  </requires>
  <extension
    point="xbmc.python.pluginsource"
    library="default.py">
   <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Play videos of different sports streaming sites</summary>
    <description>Play videos of different sports streaming sites</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.streamboxlive" name="StreamBox Live" version="1.0" provider-name="Uscclarkey">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Live Uk Tv</summary>
    <description lang="en">LIVE UK TV Channels. By using This Plugin You will receive all the uk tv for free. AWWWW YEAH!!</description>
    <disclaimer lang="en">All streams are provided by FilmOn, StreamBox Live do not host any streams!</disclaimer>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.streamboxmovies" name="StreamBox Movies" version="1.0.5" provider-name="Uscclarkey">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.metahandler" version="1.0.0"/>
    <import addon="plugin.video.youtube" version="0.0.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Stream Movies and TV Shows</summary>
    <description lang="en">Stream all the latest movies and tv shows. With no links, just click and watch. Cracking!</description>
    <disclaimer lang="en">StreamBox movies does not host any files, it simply provides you a link to the file that is easily found on the web. thank you!!</disclaimer>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.uscclarkey" name="Uscclarkey" version="1.3" provider-name="Uscclarkey">
	<extension point="xbmc.addon.repository" name="Uscclarkey">
		<info compressed="false">http://raw.github.com/uscclarkey/repo/master/addons.xml</info>
		<checksum>http://raw.github.com/uscclarkey/repo/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/uscclarkey/repo/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install MY Addons</summary>
		<description>Download and install addons from uscclarkey. For more info please visit www.streamboxlive.wordpress.com</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
